# OAuth Configuration
# Get Google credentials from: https://console.cloud.google.com/apis/credentials
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_API_KEY=your_google_api_key_here

# Get Microsoft credentials from: https://portal.azure.com/#view/Microsoft_AAD_RegisteredApps
VITE_MICROSOFT_CLIENT_ID=your_microsoft_client_id_here

# OAuth Redirect URI (update based on your deployment)
VITE_REDIRECT_URI=http://localhost:5175/oauth/callback

# Calendar Sync Settings
VITE_SYNC_INTERVAL_MINUTES=5
VITE_AUTO_START_TIMER_ENABLED=true
VITE_EVENT_NOTIFICATION_MINUTES=5
